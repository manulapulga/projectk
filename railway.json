{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "python -m pip install --upgrade pip && pip install -r requirements.txt"
  },
  "routes": [
    {
      "handle": "filesystem",
      "src": "/.well-known/(.*)"
    }
  ],
  "headers": {
    "/.well-known/assetlinks.json": {
      "Content-Type": "application/json"
    }
  }
}